Banner.Animation.prototype.start=function(){this.config.repeatDelay=3;var a=this,b=new TimelineMax(a.config),c=function(b){return(new TimelineLite).add([TweenLite.to(a.el.Price_conditions,a.s[3],{opacity:1}),TweenLite.from(a.el["Destination"+b],a.s[3],{opacity:0,left:-30}),TweenLite.from(a.el["Price_box"+b],a.s[3],{opacity:0,scale:3,ease:Expo.easeOut})]).add([TweenLite.to(a.el.Price_conditions,a.s[3],{opacity:0}),TweenLite.to(a.el["Destination"+b],a.s[3],{opacity:0,left:-80}),TweenLite.to(a.el["Price_box"+b],a.s[3],{opacity:0,left:80})],"+=2")};b.to(a.el.banner,a.s[3],{opacity:1}).to(a.el.Image,a.s[6],{left:0}).from(a.el.box,a.s[5],{width:0,height:0,padding:0}).fromTo(a.el.banner_logo,a.s[5],{bottom:-100,right:-102},{bottom:-a.el.banner_logo.clientHeight/5.5,right:-a.el.banner_logo.clientWidth/5.5,scale:.65},"-=.4").from(a.el.Title_first,a.s[3],{top:40,opacity:0}).to(a.el.Title_first,a.s[3],{top:40,opacity:0},"+=1.5").add([TweenLite.to(a.el.box,a.s[4],{height:400}),TweenLite.to(a.el.banner_logo,a.s[5],{scale:1,bottom:0,right:0}),TweenLite.from(a.el.Banner_logo_text,a.s[5],{scale:0})]).add([TweenLite.from(a.el.btn,a.s[3],{opacity:0,scale:1.4}),TweenLite.from(a.el.Travel_conditions,a.s[3],{opacity:0})]).add(a.DestinationsAnimation(c),"-="+a.s[3]).to(a.el.Travel_conditions,a.s[3],{opacity:0},"-="+a.s[3]).from(a.el.Title_second,a.s[3],{opacity:0,marginLeft:-100})},Banner.template.destinations.render=function(a){var b=new Banner.template.destinations.PriceTemplate(a),c='<div class="des-wrap abs"><div class="des-title abs" id="Destination'+a+'" data-template="simpleText" data-component="tBox"></div><div class="des-price abs" id="Price_box'+a+'" data-component="Box">'+b.template+'<div id="Starting_price'+a+'" data-template="simpleText" data-component="tBox"></div></div></div>';return c};